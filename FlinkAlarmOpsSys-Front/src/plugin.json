{
  "type": "app",
  "name": "K米-Flink配置管理",
  "id": "grafana-example-app",

  "pages": [
    { "name": "Live stream", "component": "StreamPageCtrl", "role": "Editor"},
    { "name": "Log view", "component": "LogsPageCtrl", "role": "Viewer"},
    { "name": "关键字过滤配置", "component": "KeyWordsCtrl", "role": "Editor"}
  ],

  "css": {
    "dark":  "css/dark.css",
    "light":  "css/light.css"
  },

  "info": {
    "description": "Flink配置管理模块，Grafana插件App",
    "author": {
      "name": "研发二部",
      "url": "http://raintank.io"
    },
    "keywords": ["K米-Flink配置管理"],
    "logos": {
      "small": "img/small_logo.png",
      "large": "img/large_logo.png"
    },
    "links": [
      {"name": "Project site", "url": "https://github.com/grafana/example-app"},
      {"name": "License & Terms", "url": "https://github.com/grafana/example-app/blob/master/LICENSE"}
    ],
    "version": "1.0.2",
    "updated": "2015-06-06"
  },

  "includes": [
    {"type": "dashboard", "name": "示例APP仪表盘include", "path": "dashboards/example_app_dashboard.json"},
    {"type": "panel", "name": "Example App Panel"},
    {"type": "datasource", "name": "Example App Datasource"},
    { "type": "page","name": "Live stream", "component": "StreamPageCtrl", "role": "Editor", "addToNav": true},
    { "type": "page","name": "关键字过滤配置", "component": "KeyWordsCtrl", "role": "Editor", "addToNav": true},
    {"type": "page", "name": "Log view", "component": "LogsPageCtrl", "role": "Viewer", "addToNav": true}
  ],

  "dependencies": {
    "grafanaVersion": "3.x.x",
    "plugins": []
  }
}
